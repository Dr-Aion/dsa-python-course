list1 = [0, 1, 2, 3, 4, 5, 6, 7]
list1[1] = 3
print(list1)
list1 =[7, 6, 5, 4, 3, 2, 1]
print(list1)
del list1[3:]
print(list1)
tuple1 = (0, 1, 2, 3, 4, 5, 6, 7)
print(tuple1)
tuple1 = (7, 6, 5, 4, 3, 2, 1, 0)
print(tuple1)
# del tuple1
# print(tuple1)

#Methods that can NOT be used for tuples:
#append()
#insert()
#remove()
# pop()
# clear()
# sort()
# reverse()

list1 = [(1, 2), (9, 0), (3, 4)]
print(list1)

tuple1 = ([1, 2], [9, 0], (3, 4))
print(tuple1)